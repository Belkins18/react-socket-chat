{
  "name": "react-socket-chat",
  "version": "1.0.0",
  "description": "react socket chat",
  "main": "index.ts",
  "repository": "git@github.com:Belkins18/react-socket-chat.git",
  "author": "Belibov Nikolay",
  "license": "MIT",
  "scripts": {
    "clear": "rimraf dist",
    "_build:dev": "env NODE_ENV=development webpack --config webpack.config.js",
    "_build:prod": "env NODE_ENV=production webpack --config webpack.config.js",
    "build:dev": "run-s --print-label clear _build:dev",
    "build:prod": "run-s --print-label clear _build:prod",
    "start": "env NODE_ENV=development webpack-dev-server"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "hookrouter": "1.2.3",
    "normalize.css": "^8.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@svgr/webpack": "^5.5.0",
    "@types/classnames": "^2.3.1",
    "@types/hookrouter": "^2.2.5",
    "@types/react": "^17.0.14",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.6.0",
    "css-loader": "5.0.0",
    "css-modules-typescript-loader": "^4.0.1",
    "eslint": "7.12.1",
    "eslint-config-airbnb": "18.2.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-import-resolver-webpack": "0.13.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.0",
    "lint-staged": "10.5.1",
    "node-sass": "4.12.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.1.2",
    "pretty-quick": "3.1.0",
    "sass-loader": "10.0.4",
    "style-loader": "2.0.0",
    "ts-loader": "8.0.18",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "^3.11.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css}": [
      "pretty-quick --staged",
      "eslint ./src"
    ]
  }
}
